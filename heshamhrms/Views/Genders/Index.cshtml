@model IEnumerable<heshamhrms.Models.Genders>

@{
    ViewBag.Title = "الجنس";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>الجنس</h2>

<p>
    @Html.ActionLink("إنشاء جديد", "Create")
</p>
<table class="table table-striped">
    <thead class="tablehead">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Gender_name)
            </th>
            <th>تعديل</th>
            <th>تفاصيل</th>
            <th>حذف</th>
        </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Gender_name)
                </td>
                <td>
                    @Html.ActionLink("تعديل", "Edit", new { id = item.Gender_id })
                </td>
                <td>

                    @Html.ActionLink("تفاصيل", "Details", new { id = item.Gender_id })

                </td>
                <td>
                    @Html.ActionLink("حذف", "Delete", new { id = item.Gender_id })
                </td>
            </tr>
        }

</table>
